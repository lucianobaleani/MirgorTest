<?xml version ="1.0" encoding="utf-8"?>
<odoo>

    <record id="sale_channel_list_action" model="ir.actions.act_window" >
        <field name="name">Sale Channels</field>
        <field name="res_model">sale.channel</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the First Sale Channel
            </p> 
        </field>
    </record>

    <menuitem name="Sale Channels" 
        action="sale_channel_list_action" 
        id="sale_channel_settings" 
        sequence="1" 
        parent="sale.menu_sale_config"
    />

    <record id="sale_quota_list_action" model="ir.actions.act_window" >
        <field name="name">Sale Quota</field>
        <field name="res_model">sale.quota</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the First Sale Quota
            </p> 
        </field>
    </record>

    <menuitem name="Sale Quotas" 
        action="sale_quota_list_action" 
        id="sale_quota_settings" 
        sequence="1" 
        parent="sale.prod_config_main"
    />



</odoo>
